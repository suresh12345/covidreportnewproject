{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covidreportproject-dev-adf"
		},
		"blobconnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobconnection'"
		},
		"gen2connection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'gen2connection'"
		},
		"ls_ADLSConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ADLSConnection'"
		},
		"gen2connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportadlsgen2.dfs.core.windows.net"
		},
		"ls_ADLSConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportnew8amgen2.dfs.core.windows.net"
		},
		"ls_blobconnection_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "BlobConnectionString-dev"
		},
		"ls_keyvaultconnection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://covidreportprojectdev-kv.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covidreportproject-dev-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blobconnection_connectionString": {
						"value": "[parameters('blobconnection_connectionString')]"
					},
					"gen2connection_accountKey": {
						"value": "[parameters('gen2connection_accountKey')]"
					},
					"ls_ADLSConnection_accountKey": {
						"value": "[parameters('ls_ADLSConnection_accountKey')]"
					},
					"gen2connection_properties_typeProperties_url": {
						"value": "[parameters('gen2connection_properties_typeProperties_url')]"
					},
					"ls_ADLSConnection_properties_typeProperties_url": {
						"value": "[parameters('ls_ADLSConnection_properties_typeProperties_url')]"
					},
					"ls_keyvaultconnection_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvaultconnection_properties_typeProperties_baseUrl')]"
					},
					"ls_blobconnection_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_blobconnection_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}